              <div class="tab-pane" id="my-orders">
                <div class="timeline">
                  <div class="tab-content-heading">
                    <h4>My Order</h4>
                    <% if(user.currentOrder){ %> 
                      <a target="_blank" href="/getInvoice-<%=user.currentOrder.id%>"> Get Invoice </a>
                    <%} %>
                  </div>
                  <div class="my-orders">
                    <div class="row">
                      <div class="col-lg-12 col-md-12 col-12">
                        <div class="my-checkout">
                          <div class="table-responsive">
                            <table class="table table-bordered">
                              <thead>
                                <tr>
                                  <td class="td-heading">Meal</td>
                                  <!-- <td class="td-heading">Qty</td> -->
                                  <td class="td-heading">Price</td>
                                  <td class="td-heading">Action</td>
                                </tr>
                              </thead>
                              <tbody>

                              <% if(user.currentOrder){ %> 
                                <% user.currentOrder.products.forEach( eachProduct => { %> 
                                  <tr>
                                    <td>
                                      <div class="checkout-thumb-2">
                                        <a href="productDetail-<%=eachProduct.id%>">
                                          <img
                                            src="<%=eachProduct.images[0]%>"
                                            class="img-responsive"
                                            alt="thumb"
                                            title="thumb"
                                          />
                                        </a>
                                      </div>
                                      <div class="name-dt">
                                        <a href="productDetail-<%=eachProduct.id%>"
                                          ><h4> <%= eachProduct.title %> </h4></a
                                        >
                                        <a href="restaurant_detail.html"
                                          ><p>Restaurant Name</p></a
                                        >
                                      </div>
                                    </td>
                                    <!-- <td class="td-content">2</td> -->
                                    <td class="td-content"> <%= eachProduct.price %>  </td>
                                    <td>
                                      <button
                                        class="trace-btn btn-link"
                                        data-toggle="modal"
                                        data-target="#trace"
                                      >
                                        Trace
                                      </button>
                                      <button class="invoice-btn btn-link">
                                        <i class="far fa-file-alt"></i>
                                      </button>
                                      <button class="trash-btn btn-link">
                                        <i class="far fa-trash-alt"></i>
                                      </button>
                                      <div
                                        class="modal fade"
                                        tabindex="-1"
                                        id="trace"
                                        role="dialog"
                                        aria-hidden="true"
                                      >
                                        <div class="modal-dialog modal-xl">
                                          <div class="modal-content">
                                            <div class="trace-model">
                                              <button
                                                type="button"
                                                class="close"
                                                data-dismiss="modal"
                                                aria-hidden="true"
                                              >
                                                Ã—
                                              </button>
                                              <div class="container mb-1">
                                                <div class="row no-gutters">
                                                  <div
                                                    class="col-lg-8 col-md-6 col-12"
                                                  >
                                                    <div class="trace-map">
                                                      <img
                                                        src="images/profile/trace-map.jpg"
                                                        alt=""
